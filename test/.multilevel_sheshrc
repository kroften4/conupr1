# multilevel cd/ls test
pwd
ls
ls dir
ls dir/subdir
cd dir
pwd
ls
cd ..
pwd
ls
cd dir/subdir
pwd
ls
cd /dir/subdir/../subdir/another/yet_another/../one_more
pwd
ls
cd ../../../subdir
pwd
ls
cd /

# head
head file2.json
head file2.json 5
head file2.json 12

# uniq
cat file3.txt
uniq file3.txt

# cal
cal

# cp & rm
ls
cp dir/subdir /
ls
rm -r subdir
ls
cp dir/subdir/another /dir/subdir/another2
cd dir/subdir
ls
rm -r another
ls
cd /
ls
cp file1.txt file2.md
rm file1.txt
ls
cat file2.md

# errors test
cd a b c
cd 123
ls a b c
cal a b c
cal 1 2 3 4
head -1
head a b c
uniq a b c
rm /
rm -r /
rm 123
cp a b
cp a

